node {
    stage('GetCode') {
        checkout([$class: 'GitSCM', branches: [[name: '*/master']], userRemoteConfigs: [[url: 'https://github.com/yankils/hello-world.git']]])
    }
    
    stage('SonarQube Analysis') {
    def scannerHome = tool 'sq';
    withSonarQubeEnv() {
      sh "${scannerHome}/bin/sonar-scanner -Dsonar.projectKey=multi -Dsonar.java.binaries=build/classes"
    }
  }
}
