node {
    stage('GetCode') {
        checkout([$class: 'GitSCM', branches: [[name: '*/master']], userRemoteConfigs: [[url: 'https://github.com/yankils/hello-world.git']]])
    }
    
    stage('SonarQube Analysis') {
        def scannerHome = tool name: 'sq'
        withSonarQubeEnv('sq') {
            sh "${scannerHome}/bin/sonar-scanner" +
            " -Ddonar.projectkey=multi"
        }
    }
}
